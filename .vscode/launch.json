    {
      "type": "java",
      "name": "Launch App",
      "request": "launch",
      "mainClass": "org.example.App",
      "projectName": "wordcount"
    },
    {
      "type": "java",
      "name": "Launch WordCount",
      "request": "launch",
      "mainClass": "org.example.WordCount",
      "projectName": "wordcount",
      "args": "input output & rm -rf output"
    }
  ]
